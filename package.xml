     generated package.xml files -->
<package version="1.0">
  <name>XML_RPC</name>
  <summary>A PEAR-ified version of Useful inc's xmlrpc implementation for PHP.</summary>
  <maintainers>
    <maintainer>
      <user>SSB</user>
      <name>Stig Sæther Bakken</name>
      <email>stig@php.net</email>
    </maintainer>
  </maintainers>
  <release>
    <version>1.0.1</version>
    <date>2001-09-25</date>
    <notes><![CDATA[This is a PEAR-ified version of Useful Inc's 1.0.1 release.
